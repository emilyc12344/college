#!/usr/bin/env python3

from math import sqrt

class Point(object):

    def __init__(self, x=0, y=0):
        self.x = x
        self.y = y

    def midpoint(self, other):
        midx = (self.x + self.x) // 2
        midy = (self.y + self.y) // 2
        return midx, midy

    def distance(self, other):
        return sqrt(((other.x - self.x) ** 2))
